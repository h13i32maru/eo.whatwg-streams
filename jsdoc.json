{
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "source": {
    "_include": "./interface/",
    "include": "./interface/sample.d.js"
  },
  "opts": {
    "_template": "./misc/jsdoc/default",
    "_destination": "./out/doc/",
    "template": "./misc/jsdoc/jsdoc-cloudy",
    "destination": "./out/doc-new/",
    "recurse": true,
    "readme": "./README.md",
    "package": "./package.json"
  }
}
